<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pavovera.petagram">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_mascotas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_mascotas_redondo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="co.siacademica.mascotasS3.FavMascotas"
            android:parentActivityName="co.siacademica.mascotasS3.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="co.siacademica.mascotasS3.MainActivity" />
        </activity>
        <activity android:name="co.siacademica.mascotasS3.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>